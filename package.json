{
  "name": "performance-monitoring-system",
  "version": "1.0.0",
  "description": "前端PMS-性能监控系统",
  "author": "Mason.meng",
  "scripts": {
    "↓↓↓↓↓↓↓↓↓↓↓↓ 全部启动 ↓↓↓↓↓↓↓↓↓↓↓↓": "————————————————彩蛋：————————————————————",
    "dev": "cross-env PMS_MODE=dev npm-run-all --parallel dev:*",
    "build": "cross-env PMS_MODE=prod npm-run-all --serial build:*",
    "i": "yarn un && yarn in",
    "in": "yarn add npm-run-all --dev && npm-run-all --serial in:*",
    "un": "yarn add npm-run-all --dev && npm-run-all --serial un:*",
    "↓↓↓↓↓↓↓↓↓↓↓↓ 逐一启动 ↓↓↓↓↓↓↓↓↓↓↓↓": "————————————————老头好，老头有低保！————————————————",
    "dev:main": "cd src/main && yarn start",
    "dev:react": "cd src/react && yarn start",
    "dev:vue": "cd src/vue && yarn start",
    "build:main": "cd src/main && yarn build",
    "build:react": "cd src/react && yarn build",
    "build:vue": "cd src/vue && yarn build",
    "in:pms": "yarn",
    "in:main": "cd src/main && yarn",
    "in:react": "cd src/react && yarn",
    "in:vue": "cd src/vue && yarn",
    "un:main": "cd src/main && rm -rf node_modules/",
    "un:react": "cd src/react && rm -rf node_modules/",
    "un:vue": "cd src/vue && rm -rf node_modules/",
    "un:pms": "rm -rf node_modules/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@rescripts/cli": "^0.0.14",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.13.0",
    "less-loader": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.1.0",
    "react-scripts": "^3.4.1",
    "style-loader": "^1.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "core-js": "^3.6.5",
    "echarts": "^5.0.0",
    "path": "^0.12.7",
    "qiankun": "^2.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "tiny-cookie": "^2.3.2",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-beta.11",
    "vuex": "^4.0.0-beta.4"
  }
}
